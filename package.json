{
  "name": "softbar",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "start": "node dist/apps/api/main.js",
    "build": "nx run main-app:build:production && nx run api:build:production",
    "upgrade": "FORCE_NG_UPDATE=true nx migrate latest",
    "test": "nx test",
    "clean": "rm -rf .angular dist node_modules package-lock.json pnpm-lock.yaml",
    "clean:npm": "npm cache clean --force",
    "clean:pnpm": "pnpm store prune",
    "auto-commit-push": "git add . && git commit -m \"`echo \"Auto commit by $(git config user.name) at $(date +%Y-%m-%dT%H:%M%z)\"`\" &&git push origin $(git branch --show-current)",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist/apps/main-app",
    "herokuPush": "git add . && git commit -m \"`echo \"Auto commit by $(git config user.name) at $(date +%Y-%m-%dT%H:%M%z)\"`\" &&git push heroku HEAD:master "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.1.5",
    "@angular/cdk": "19.1.3",
    "@angular/common": "19.1.5",
